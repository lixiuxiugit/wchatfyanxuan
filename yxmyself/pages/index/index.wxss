/**index.wxss**/

/* 搜索 */

.i-search image {
  width: 28rpx;
  height: 28rpx;
  margin-left: 10rpx;
}

.i-search input {
  padding-left: 10px;
  font-size: 26rpx;
}

.gdmap {
  width: 115rpx;
  font-size: 20rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.search {
  display: flex;
  align-items: center;
  padding: 0rpx 30rpx;
}

.i-search {
  flex: 1;
  display: flex;
  align-items: center;
  background: #ededed;
  border-radius: 8rpx;
  margin-left: 15rpx;
}

/* 轮播 */

.banner {
  margin-top: 8rpx;
}

.sw image {
  width: 100%;
  height: 417rpx;
}

/* 家居导航 */

.jiaju {
  display: flex;
  padding: 24rpx 0rpx;
}

.jiaju image {
  width: 58rpx;
  height: 58rpx;
}

.jiaju text {
  font-size: 28rpx;
  color: #333;
  padding-top: 8rpx;
}

.jiaju-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  flex: 1;
}

/* 品牌制造商直供 */

.brand {
  display: flex;
  flex-direction: column;
}

.bran-title {
  text-align: center;
  font-size: 28rpx;
  color: #333;
  padding: 40rpx 0rpx;
  border-top: 20rpx solid #f4f4f4;
}

.brandlist {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-evenly;
}

.brand-item {
  position: relative;
}

.brand-item image {
  width: 360rpx;
  height: 235rpx;
}

.bran-discribe {
  display: flex;
  flex-direction: column;
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  z-index: 999;
}

.bran-discribe text {
  font-size: 28rpx;
  color: #333;
}

/* 新品首发 */

.newgodsList {
  white-space: nowrap;
  /* background: red; */
}

.newgods-item {
  display: inline-block;
  border-right: solid 1px #f4f4f4;
}

.newgods-item text {
  width: 280rpx;
  display: block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  text-indent: 1rem;
}

.ng-title {
  font-size: 30rpx;
  font-weight: bold;
}

.ng-subtitle {
  color: #8a8a8a;
  font-size: 24rpx;
  padding: 12rpx 0rpx;
}

.ng-price {
  color: #9c3232;
  font-size: 24rpx;
}

.newgods-item image {
  width: 280rpx;
  height: 280rpx;
}

.negods-title {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: url(data:image/png;base64,) no-repeat;
  padding: 100rpx 0rpx;
  border-top: 20rpx solid #f4f4f4;
  border-bottom: 20rpx solid #f4f4f4;
}

.negods-title text {
  display: block;
  color: #8c9bae;
}

.ngsf {
  font-size: 32rpx;
}

.ngqb {
  width: 180rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin: 27rpx auto 0 auto;
  font-size: 22rpx;
  background: #d8e4f0;
  text-align: center;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* 人气推广好物精选  */

.hotgoods {
  background: url(data:image/png;base64,) no-repeat;
}

.hotjx {
  background: #f4e9cb;
}

/* 专题精选 */

.topicsel {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  border-top: 20rpx solid #f4f4f4;
  padding: 30rpx 0rpx;
}

.topicsel image {
  display: block;
  width: 36rpx;
  height: 36rpx;
  margin-top: 6rpx;
  margin-left: 10rpx;
}

.topicjx {
  margin-left: 24rpx;
  border-right: none;
}

.topicjx:last-child {
  margin-right: 24rpx;
}

.topicjx image {
  width: 585rpx;
  height: 330rpx;
  border-radius: 15rpx;
}

.topicjx text {
  text-indent: 0rem;
}

.topicjx-disc {
  display: flex;
  justify-content: space-between;
}

.topicjx-price {
  text-align: right;
}

/* 家居好物 */

.newCate {
  background: #f4f4f4;
  padding-bottom: 20rpx;
}

.newCate-item {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.newCate-title {
  text-align: center;
  font-size: 28rpx;
  color: #333;
  padding: 30rpx 0rpx;
}

.newCate-item-total {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 96%;
}

.newCate-list {
  display: flex;
  flex-direction: column;
  width: 49%;
  background: #fff;
  margin-bottom: 14rpx;
  align-items: center;
}

.newCate-list:last-child {
  margin-bottom: 0rpx;
}

.newCate-list text {
  display: block;
  width: 100%;
  text-indent: 1rem;
}

.newCate-list image {
  width: 302rpx;
  height: 302rpx;
}

.newCate-list text {
  font-size: 28rpx;
}

.newCate-list-name {
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-top: 6rpx;
}

.newCate-list-price {
  color: #9c3232;
  padding-bottom: 20rpx;
}
.lastlist{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 395rpx;
}
.lastlist text {
  color: #333;
  font-size: 33rpx;
  text-align: center;
  text-indent: 0rem;
}

.lastlist image {
  width: 70rpx;
  height: 70rpx;
}
